<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite  name="mWallet Suite" verbose="1" preserve-order="true" parallel="true">
    <test name="Back Office User" >
        <parameter name="browser" value="Chrome"></parameter>
        <groups>
            <run>
                <include name="BackOfficeUsers"></include>
                <include name="Merchants"/>
            </run>
        </groups>
        <classes>
            <class name="Tests.Smoke.BackOfficeUserLogIn.TestLogIn"></class>
            <class name="Tests.Smoke.Merchants.TestMerchantCreation"></class>
        </classes>
    </test>

</suite>